{
  "name": "paypal",
  "version": "1.0.0",
  "description": "A PayPal Applications Engineer coding challenge.",
  "main": "server.js",
  "scripts": {
    "react": "env UV_THREADPOOL_SIZE=100 webpack-dev-server --progress --colors",
    "node": "nodemon server",
    "start": "yarn react | yarn node",
    "wake": "atom . && open http://localhost:8080 && yarn start",
    "build": "webpack -p --progress --colors",
    "clean": "rm -rf node_modules && yarn",
    "lint": "eslint app/.; exit 0",
    "fix": "eslint --fix app/.; exit 0",
    "images": "find ./dist/assets -name '*.svg' -type f -exec svgo '{}' \\;"
  },
  "author": "Zac Arellano",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.14.1",
    "nodemon": "^1.11.0",
    "payment-icons": "^0.0.12",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.3",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "browser-sync": "^2.18.7",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.2.2",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-stats-plugin": "^0.1.4",
    "webpack-visualizer-plugin": "^0.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacarellano/paypal.git"
  },
